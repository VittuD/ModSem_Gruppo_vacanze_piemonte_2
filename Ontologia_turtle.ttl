#################################################################

###  http://purl.org/dc/elements/1.1/Date
dc:Date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/Title
dc:Title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://schema.org/relatedLink
schema:relatedLink rdf:type owl:AnnotationProperty .


###  http://web.resource.org/cc/license
<http://web.resource.org/cc/license> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaEdizione
<http://www.semanticweb.org/owl/owlapi/turtle#HaEdizione> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaGenereLetterario
<http://www.semanticweb.org/owl/owlapi/turtle#HaGenereLetterario> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaProvenienzaAutore
<http://www.semanticweb.org/owl/owlapi/turtle#HaProvenienzaAutore> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#parteDiAntologia
<http://www.semanticweb.org/owl/owlapi/turtle#parteDiAntologia> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#parteDiSaga
<http://www.semanticweb.org/owl/owlapi/turtle#parteDiSaga> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broadMatch
skos:broadMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty ;
                rdfs:subPropertyOf skos:closeMatch .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaDataPubblicazione
lwt:HaDataPubblicazione rdf:type owl:AnnotationProperty ;
                        rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaLingua
lwt:HaLingua rdf:type owl:AnnotationProperty .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaNome
lwt:HaNome rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#ScrittoDa
<http://www.semanticweb.org/owl/owlapi/turtle#ScrittoDa> rdf:type owl:ObjectProperty ;
                                                         owl:inverseOf lwt:AutoreDi ;
                                                         rdfs:domain [ rdf:type owl:Class ;
                                                                       owl:unionOf ( lwt:Edizione
                                                                                     lwt:OperaLetteraria
                                                                                   )
                                                                     ] ;
                                                         rdfs:range lwt:Autore .


###  https://litetaryworktopics.com/litetaryworktopics2025#AppartieneA
lwt:AppartieneA rdf:type owl:ObjectProperty ;
                owl:inverseOf lwt:Contiene .


###  https://litetaryworktopics.com/litetaryworktopics2025#AutoreDi
lwt:AutoreDi rdf:type owl:ObjectProperty ;
             rdfs:domain lwt:Autore ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( lwt:Edizione
                                        lwt:OperaLetteraria
                                      )
                        ] .


###  https://litetaryworktopics.com/litetaryworktopics2025#Contiene
lwt:Contiene rdf:type owl:ObjectProperty .


###  https://litetaryworktopics.com/litetaryworktopics2025#Contribuisce
lwt:Contribuisce rdf:type owl:ObjectProperty ;
                 owl:inverseOf lwt:HaContributoDi ;
                 rdfs:domain lwt:Collaboratore ;
                 rdfs:range lwt:Edizione .


###  https://litetaryworktopics.com/litetaryworktopics2025#EdizioneDi
lwt:EdizioneDi rdf:type owl:ObjectProperty ;
               owl:inverseOf lwt:HaEdizione ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain lwt:Edizione ;
               rdfs:range lwt:OperaLetteraria .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaAutore
lwt:HaAutore rdf:type owl:ObjectProperty ;
             rdfs:domain lwt:Autore ;
             rdfs:range lwt:OperaLetteraria .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaContributoDi
lwt:HaContributoDi rdf:type owl:ObjectProperty ;
                   rdfs:domain lwt:Edizione ;
                   rdfs:range lwt:Collaboratore .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaEdizione
lwt:HaEdizione rdf:type owl:ObjectProperty ;
               rdfs:domain lwt:OperaLetteraria ;
               rdfs:range lwt:Edizione .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaProvenienza
lwt:HaProvenienza rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( <http://www.w3.org/ns/prov#Agent>
                                              <http://www.w3.org/ns/prov#Organization>
                                            )
                              ] ;
                  rdfs:range lwt:Nazione .


###  https://litetaryworktopics.com/litetaryworktopics2025#LavoraPerCasaEditrice
lwt:LavoraPerCasaEditrice rdf:type owl:ObjectProperty ;
                          owl:inverseOf lwt:haEditore ;
                          rdfs:domain lwt:Editore ;
                          rdfs:range lwt:CasaEditrice .


###  https://litetaryworktopics.com/litetaryworktopics2025#Pubblica
lwt:Pubblica rdf:type owl:ObjectProperty ;
             owl:inverseOf lwt:PubblicatoDa ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( <http://www.w3.org/ns/prov#Organization>
                                         lwt:Persona
                                       )
                         ] ;
             rdfs:range lwt:Edizione .


###  https://litetaryworktopics.com/litetaryworktopics2025#PubblicaComeOrganizzazione
lwt:PubblicaComeOrganizzazione rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf lwt:Pubblica ;
                               rdfs:domain <http://www.w3.org/ns/prov#Organization> ;
                               rdfs:range lwt:Edizione .


###  https://litetaryworktopics.com/litetaryworktopics2025#PubblicaComeSingolo
lwt:PubblicaComeSingolo rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf lwt:Pubblica ;
                        owl:inverseOf lwt:PubblicataDallaPersona ;
                        rdfs:domain lwt:Persona ;
                        rdfs:range lwt:Edizione .


###  https://litetaryworktopics.com/litetaryworktopics2025#PubblicataDallOrganizazione
lwt:PubblicataDallOrganizazione rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf lwt:PubblicatoDa ;
                                rdfs:domain lwt:Edizione ;
                                rdfs:range <http://www.w3.org/ns/prov#Organization> .


###  https://litetaryworktopics.com/litetaryworktopics2025#PubblicataDallaPersona
lwt:PubblicataDallaPersona rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf lwt:PubblicatoDa ;
                           rdfs:domain lwt:Edizione ;
                           rdfs:range lwt:Persona .


###  https://litetaryworktopics.com/litetaryworktopics2025#PubblicatoDa
lwt:PubblicatoDa rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain lwt:Edizione ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( <http://www.w3.org/ns/prov#Organization>
                                            lwt:Persona
                                          )
                            ] .


###  https://litetaryworktopics.com/litetaryworktopics2025#VieneClassificatoCome
lwt:VieneClassificatoCome rdf:type owl:ObjectProperty ;
                          owl:inverseOf lwt:VieneUtilizzatoIn ;
                          rdfs:domain [ rdf:type owl:Class ;
                                        owl:unionOf ( lwt:Edizione
                                                      lwt:OperaLetteraria
                                                    )
                                      ] ;
                          rdfs:range [ rdf:type owl:Class ;
                                       owl:unionOf ( lwt:Genere
                                                     lwt:Tema
                                                   )
                                     ] .


###  https://litetaryworktopics.com/litetaryworktopics2025#VieneUtilizzatoIn
lwt:VieneUtilizzatoIn rdf:type owl:ObjectProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( lwt:Genere
                                                  lwt:Tema
                                                )
                                  ] ;
                      rdfs:range [ rdf:type owl:Class ;
                                   owl:unionOf ( lwt:Edizione
                                                 lwt:OperaLetteraria
                                               )
                                 ] .


###  https://litetaryworktopics.com/litetaryworktopics2025#haEditore
lwt:haEditore rdf:type owl:ObjectProperty ;
              rdfs:domain lwt:CasaEditrice ;
              rdfs:range lwt:Editore .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/elements/1.1/Date
dc:Date rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:date .


###  http://purl.org/dc/elements/1.1/Title
dc:Title rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaDataPubblicazione
lwt:HaDataPubblicazione rdf:type owl:DatatypeProperty ;
                        rdfs:range xsd:date .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaDescrizione
lwt:HaDescrizione rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaISBN
lwt:HaISBN rdf:type owl:DatatypeProperty ;
           rdfs:domain lwt:Edizione ;
           rdfs:range xsd:nonPositiveInteger .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaLingua
lwt:HaLingua rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:language .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaNome
lwt:HaNome rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:string .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaNumeroPagine
lwt:HaNumeroPagine rdf:type owl:DatatypeProperty ;
                   rdfs:domain lwt:Edizione ;
                   rdfs:range xsd:integer .


###  https://litetaryworktopics.com/litetaryworktopics2025#HaRuolo
lwt:HaRuolo rdf:type owl:DatatypeProperty ;
            rdfs:domain lwt:Collaboratore ;
            rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/List
<http://dbpedia.org/resource/List> rdf:type owl:Class ;
                                   rdfs:comment "Una lista è una raccolta di elementi, che può contenere oggetti o entità con caratteristiche simili o correlate, come una serie di libri, opere o altri elementi definiti da un tema comune."@it ;
                                   rdfs:label """List
"""@it .


###  http://purl.org/dc/terms/Location
<http://purl.org/dc/terms/Location> rdf:type owl:Class ;
                                    rdfs:subClassOf <http://purl.org/vocab/frbr/core#Place> ;
                                    rdfs:comment "Un Luogo Fisico rappresenta un Luogo fisicamente presente, tramite la denominazione corrente o passata."@it ;
                                    rdfs:label "LuogoFisico"@it ;
                                    skos:closeMatch "dc:Location"@en .


###  http://purl.org/vocab/frbr/core#Place
<http://purl.org/vocab/frbr/core#Place> rdf:type owl:Class ;
                                        rdfs:comment """(Rappresenta un'entità spaziale generica, come un luogo o una posizione.)

"""@it ;
                                        rdfs:label "Luogo"@it ;
                                        skos:closeMatch "frbr:Place"@en .


###  http://www.semanticweb.org/owl/owlapi/turtle#GrandeSaga
<http://www.semanticweb.org/owl/owlapi/turtle#GrandeSaga> rdf:type owl:Class ;
                                                          owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                                owl:onProperty lwt:Contiene ;
                                                                                owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                                                                owl:onClass lwt:OperaLetteraria
                                                                              ] ;
                                                          rdfs:subClassOf lwt:Saga ;
                                                          rdfs:comment "Una grande saga è una narrazione estesa e complessa che si sviluppa su molteplici opere letterarie, spesso seguendo diversi gruppi di personaggi o intere famiglie, con una trama che si estende su generazioni o eventi di ampia portata."@it ;
                                                          rdfs:label """GrandeSaga
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Trilogia
<http://www.semanticweb.org/owl/owlapi/turtle#Trilogia> rdf:type owl:Class ;
                                                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                                                              owl:onProperty lwt:Contiene ;
                                                                              owl:qualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                                                              owl:onClass lwt:OperaLetteraria
                                                                            ] ;
                                                        rdfs:subClassOf lwt:Saga ;
                                                        rdfs:comment """Una trilogia è una saga composta da tre opere letterarie che si sviluppano attraverso una trama continuativa, generalmente seguendo lo stesso gruppo di personaggi o un arco narrativo unico.


Funziona solo se sono definiti con enumerazione 
"""@it ;
                                                        rdfs:label """Trilogia


"""@it .


###  http://www.w3.org/ns/prov#Agent
<http://www.w3.org/ns/prov#Agent> rdf:type owl:Class ;
                                  rdfs:comment "Un agente è un'entità che ha un ruolo attivo in un contesto sociale o lavorativo. Può essere un'organizzazione o una persona che svolge attività specifiche come creare, pubblicare o distribuire opere."@it ;
                                  rdfs:label "Agent"@it ;
                                  skos:exactMatch "dc:Agent"@en .


###  http://www.w3.org/ns/prov#Organization
<http://www.w3.org/ns/prov#Organization> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
                                         rdfs:comment "Un'organizzazione è un'entità strutturata che agisce come un singolo soggetto, come una casa editrice, un'impresa o un ente, ed è composta da persone che operano insieme per raggiungere obiettivi comuni."@it ;
                                         rdfs:label "Organization"@it ;
                                         skos:broadMatch "frbr:CorporateBody"@en .


###  https://litetaryworktopics.com/litetaryworktopics2025#Autore
lwt:Autore rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty lwt:AutoreDi ;
                                 owl:someValuesFrom lwt:Edizione
                               ] ;
           rdfs:subClassOf lwt:Persona ;
           rdfs:comment "Un autore è una persona che scrive un'opera letteraria, scientifica o artistica, ed è responsabile della creazione e dell'originalità del contenuto dell'opera."@it ;
           rdfs:label """Autore
"""@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#AutoreIndipendente
lwt:AutoreIndipendente rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( lwt:Autore
                                                                  lwt:Editore
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty lwt:AutoreDi ;
                                                                    owl:someValuesFrom lwt:Edizione
                                                                  ]
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty lwt:Pubblica ;
                                                                    owl:someValuesFrom lwt:Edizione
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf lwt:Autore ;
                       rdfs:comment "Un autore indipendente è uno scrittore che pubblica e distribuisce le proprie opere senza l'intermediazione di una casa editrice tradizionale, gestendo autonomamente l'intero processo editoriale e promozionale. Questo approccio consente maggiore libertà creativa e guadagni diretti."@it ;
                       rdfs:label """AutoreIndipendente
"""@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#Canzoniere
lwt:Canzoniere rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty lwt:Contiene ;
                                     owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                     owl:onClass lwt:Poesia
                                   ] ;
               rdfs:subClassOf lwt:CollanaEditoriale ;
               rdfs:comment "Una raccolta di poesie che appartengono alla categoria \"Poesia\". Ogni elemento della collezione è un'istanza di \"Poesia\"."@it ;
               rdfs:label "Canzoniere"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#CasaEditrice
lwt:CasaEditrice rdf:type owl:Class ;
                 owl:equivalentClass [ owl:intersectionOf ( <http://www.w3.org/ns/prov#Organization>
                                                            [ rdf:type owl:Restriction ;
                                                              owl:onProperty lwt:Pubblica ;
                                                              owl:someValuesFrom lwt:Edizione
                                                            ]
                                                          ) ;
                                       rdf:type owl:Class
                                     ] ;
                 rdfs:subClassOf <http://www.w3.org/ns/prov#Organization> ;
                 rdfs:comment "Una casa editrice è un'organizzazione che si occupa della pubblicazione e distribuzione di opere letterarie, scientifiche o artistiche, curando la produzione, la stampa e la diffusione dei libri."@it ;
                 rdfs:label "CasaEditrice"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#Collaboratore
lwt:Collaboratore rdf:type owl:Class ;
                  rdfs:subClassOf lwt:Persona ;
                  rdfs:comment "Persona che ha contribuito alla Scrittura di un'Edizione non come Autore"@it ;
                  rdfs:label "Collaboratore"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#CollanaEditoriale
lwt:CollanaEditoriale rdf:type owl:Class ;
                      rdfs:subClassOf lwt:ListaDiOpereLetterarie ;
                      rdfs:comment "Una raccolta di testi pubblicati da un editore con criteri comuni di genere, tema o formato."@it ;
                      rdfs:label "CollanaEditoriale"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#Editore
lwt:Editore rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( lwt:Persona
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty lwt:Pubblica ;
                                                         owl:someValuesFrom lwt:Edizione
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:subClassOf lwt:Persona ;
            rdfs:comment """Persona che ha pubblicato un Edizione
"""@it ;
            rdfs:label """Editore
"""@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#Edizione
lwt:Edizione rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty lwt:EdizioneDi ;
                                   owl:someValuesFrom lwt:OperaLetteraria
                                 ] ;
             rdfs:comment "Un'entità che rappresenta una specifica pubblicazione di un'opera letteraria."@it ;
             rdfs:label "Edizione"@it ;
             skos:broadMatch "frbr:Endeavour"@en .


###  https://litetaryworktopics.com/litetaryworktopics2025#Genere
lwt:Genere rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( lwt:Fantasy
                                             lwt:Romanzo_storico
                                           )
                               ] ;
           rdfs:comment "Il genere di un'opera letteraria è una categoria che raggruppa libri con caratteristiche tematiche o stilistiche simili, come ad esempio fantasy, giallo, romance, storia, horror, ecc. Ogni genere aiuta a identificare il tono, il contenuto e lo stile dell'opera."@it ;
           rdfs:label "Genere"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#ListaDiOpereLetterarie
lwt:ListaDiOpereLetterarie rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty lwt:Contiene ;
                                                 owl:someValuesFrom lwt:OperaLetteraria
                                               ] ;
                           rdfs:subClassOf <http://dbpedia.org/resource/List> .


###  https://litetaryworktopics.com/litetaryworktopics2025#Nazione
lwt:Nazione rdf:type owl:Class ;
            rdfs:subClassOf <http://purl.org/dc/terms/Location> ;
            rdfs:comment "Un'entità nazionale nel presente o passato."@it ;
            rdfs:label "Nazione"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#OperaLetteraria
lwt:OperaLetteraria rdf:type owl:Class ;
                    rdfs:comment "L'entità concettuale di Opera Letteraria, include tutte le sue materializzazioni e Formati."@it ;
                    rdfs:label "OperaLetteraria"@it ;
                    skos:closeMatch "frbr:Work"@en .


###  https://litetaryworktopics.com/litetaryworktopics2025#OperaTeatrale
lwt:OperaTeatrale rdf:type owl:Class ;
                  rdfs:subClassOf lwt:OperaLetteraria ;
                  rdfs:comment "Un'opera teatrale è una forma di opera letteraria scritta per essere rappresentata su un palcoscenico, con personaggi che parlano e interagiscono tra loro secondo una trama."@it ;
                  rdfs:label "Operateatrale"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#Persona
lwt:Persona rdf:type owl:Class ;
            rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
            rdfs:comment "Una persona è un individuo umano che svolge un ruolo all'interno di un contesto sociale o professionale, come autore, editore, o altro."@it ;
            rdfs:label "Persona"@it ;
            skos:exactMatch "frbr:Agent"@en .


###  https://litetaryworktopics.com/litetaryworktopics2025#Personaggio
lwt:Personaggio rdf:type owl:Class ;
                rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
                rdfs:comment " Una persona, creatura magica o entità"@it ;
                rdfs:label "Personaggio"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#PersonaggioLibro
lwt:PersonaggioLibro rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty lwt:AppartieneA ;
