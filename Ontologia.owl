@prefix : <http://www.semanticweb.org/owl/owlapi/turtle#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix geo: <https://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <https://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dbpedia: <https://dbpedia.org/ontology/> .
@base <http://www.semanticweb.org/owl/owlapi/turtle#> .

<https://litetaryworktopics.com/litetaryworktopics2025#> rdf:type owl:Ontology ;
                                                          dc:creator "Davide Vitturini"@en ,
                                                                     "Enrico Chiesa"@en ;
                                                          dc:date "2025-01-29" ;
                                                          dc:description "Questa ontologia ha l'obiettivo di fornire una rappresentazione generale sulle principali tematiche trattate dalla letteratura, con il particolare obiettivo di rendere possibile il suggerimento di nuove da parte di esperti o amatori."@it ;
                                                          vann:preferredNamespacePrefix "lwt" ;
                                                          vann:preferredNamespaceUri "https://litetaryworktopics.com/litetaryworktopics2025#"@en ;
                                                          <http://web.resource.org/cc/license> "MIT" ;
                                                          rdfs:label "litetaryworktopics2025"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty ;
        rdfs:range xsd:date .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty ;
         rdfs:range xsd:string .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://web.resource.org/cc/license
<http://web.resource.org/cc/license> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaEdizione
:HaEdizione rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaGenereLetterario
:HaGenereLetterario rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaLingua
:HaLingua rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaProvenienzaAutore
:HaProvenienzaAutore rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#haDataPubblicazione
:haDataPubblicazione rdf:type owl:AnnotationProperty ;
                     rdfs:range xsd:date .


###  http://www.semanticweb.org/owl/owlapi/turtle#haNome
:haNome rdf:type owl:AnnotationProperty ;
        rdfs:range xsd:string .


###  http://www.semanticweb.org/owl/owlapi/turtle#parteDiAntologia
:parteDiAntologia rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#parteDiSaga
:parteDiSaga rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broadMatch
skos:broadMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
skos:closeMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty ;
                rdfs:subPropertyOf skos:closeMatch .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/owl/owlapi/turtle#AppartieneA
:AppartieneA rdf:type owl:ObjectProperty ;
             owl:inverseOf :Contiene .


###  http://www.semanticweb.org/owl/owlapi/turtle#AutoreDi
:AutoreDi rdf:type owl:ObjectProperty ;
          owl:inverseOf :ScrittoDa ;
          rdfs:domain :Autore ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                     <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                   )
                     ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#Contiene
:Contiene rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#Contribuisce
:Contribuisce rdf:type owl:ObjectProperty ;
              owl:inverseOf :HaContributoDi ;
              rdfs:domain :Collaboratore ;
              rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  http://www.semanticweb.org/owl/owlapi/turtle#EdizioneDi
:EdizioneDi rdf:type owl:ObjectProperty ;
            owl:inverseOf <https://litetaryworktopics.com/litetaryworktopics2025##HaEdizione> ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
            rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaAutore
:HaAutore rdf:type owl:ObjectProperty ;
          rdfs:domain :Autore ;
          rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaContributoDi
:HaContributoDi rdf:type owl:ObjectProperty ;
                rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
                rdfs:range :Collaboratore .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaProvenienza
:HaProvenienza rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( <http://www.w3.org/ns/prov#Agent>
                                           <http://www.w3.org/ns/prov#Organization>
                                         )
                           ] ;
               rdfs:range :Nazione .


###  http://www.semanticweb.org/owl/owlapi/turtle#HaTema
:HaTema rdf:type owl:ObjectProperty ;
        rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
        rdfs:range :Tema .


###  http://www.semanticweb.org/owl/owlapi/turtle#LavoraPerCasaEditrice
:LavoraPerCasaEditrice rdf:type owl:ObjectProperty ;
                       owl:inverseOf :haEditore ;
                       rdfs:domain :Editore ;
                       rdfs:range :CasaEditrice .


###  http://www.semanticweb.org/owl/owlapi/turtle#Pubblica
:Pubblica rdf:type owl:ObjectProperty ;
          owl:inverseOf :PubblicatoDa ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Persona
                                      <http://www.w3.org/ns/prov#Organization>
                                    )
                      ] ;
          rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  http://www.semanticweb.org/owl/owlapi/turtle#PubblicaComeOrganizzazione
:PubblicaComeOrganizzazione rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :Pubblica ;
                            rdfs:domain <http://www.w3.org/ns/prov#Organization> ;
                            rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  http://www.semanticweb.org/owl/owlapi/turtle#PubblicaComeSingolo
:PubblicaComeSingolo rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :Pubblica ;
                     owl:inverseOf :PubblicataDallaPersona ;
                     rdfs:domain :Persona ;
                     rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  http://www.semanticweb.org/owl/owlapi/turtle#PubblicataDallOrganizazione
:PubblicataDallOrganizazione rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :PubblicatoDa ;
                             rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
                             rdfs:range <http://www.w3.org/ns/prov#Organization> .


###  http://www.semanticweb.org/owl/owlapi/turtle#PubblicataDallaPersona
:PubblicataDallaPersona rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :PubblicatoDa ;
                        rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
                        rdfs:range :Persona .


###  http://www.semanticweb.org/owl/owlapi/turtle#PubblicatoDa
:PubblicatoDa rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( :Persona
                                         <http://www.w3.org/ns/prov#Organization>
                                       )
                         ] .


###  http://www.semanticweb.org/owl/owlapi/turtle#ScrittoDa
:ScrittoDa rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                       <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                     )
                       ] ;
           rdfs:range :Autore .


###  http://www.semanticweb.org/owl/owlapi/turtle#VieneUtilizzatoIn
:VieneUtilizzatoIn rdf:type owl:ObjectProperty ;
                   owl:inverseOf <https://litetaryworktopics.com/litetaryworktopics2025##HaGenereLetterario> ;
                   rdfs:domain :Genere ;
                   rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  http://www.semanticweb.org/owl/owlapi/turtle#haEditore
:haEditore rdf:type owl:ObjectProperty ;
           rdfs:domain :CasaEditrice ;
           rdfs:range :Editore .


###  https://litetaryworktopics.com/litetaryworktopics2025##HaEdizione
<https://litetaryworktopics.com/litetaryworktopics2025##HaEdizione> rdf:type owl:ObjectProperty ;
                                                                    rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
                                                                    rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  https://litetaryworktopics.com/litetaryworktopics2025##HaGenereLetterario
<https://litetaryworktopics.com/litetaryworktopics2025##HaGenereLetterario> rdf:type owl:ObjectProperty ,
                                                                                     owl:FunctionalProperty ;
                                                                            rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#haDataPubblicazione
:haDataPubblicazione rdf:type owl:DatatypeProperty ;
                     rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  http://www.semanticweb.org/owl/owlapi/turtle#haDescrizione
:haDescrizione rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/owl/owlapi/turtle#haISBN
:haISBN rdf:type owl:DatatypeProperty ;
        rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
        rdfs:range xsd:nonPositiveInteger .


###  http://www.semanticweb.org/owl/owlapi/turtle#haLingua
:haLingua rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:language .


###  http://www.semanticweb.org/owl/owlapi/turtle#haNome
:haNome rdf:type owl:DatatypeProperty .


###  http://www.semanticweb.org/owl/owlapi/turtle#haPagine
:haPagine rdf:type owl:DatatypeProperty ;
          rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
          rdfs:range xsd:integer .


###  https://litetaryworktopics.com/litetaryworktopics2025#haRuolo
:haRuolo rdf:type owl:DatatypeProperty ;
         rdfs:domain :Collaboratore ;
         rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/List
<http://dbpedia.org/resource/List> rdf:type owl:Class ;
                                   rdfs:comment "Una lista è una raccolta di elementi, che può contenere oggetti o entità con caratteristiche simili o correlate, come una serie di libri, opere o altri elementi definiti da un tema comune."@it ;
                                   rdfs:label """List
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Autore
:Autore rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Restriction ;
                              owl:onProperty :AutoreDi ;
                              owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                            ] ;
        rdfs:subClassOf :Persona ;
        rdfs:comment "Un autore è una persona che scrive un'opera letteraria, scientifica o artistica, ed è responsabile della creazione e dell'originalità del contenuto dell'opera."@it ;
        rdfs:label """Autore
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#AutoreIndipendente
:AutoreIndipendente rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Autore
                                                               :Editore
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :AutoreDi ;
                                                                 owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :Pubblica ;
                                                                 owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Autore ;
                    rdfs:comment "Un autore indipendente è uno scrittore che pubblica e distribuisce le proprie opere senza l'intermediazione di una casa editrice tradizionale, gestendo autonomamente l'intero processo editoriale e promozionale. Questo approccio consente maggiore libertà creativa e guadagni diretti."@it ;
                    rdfs:label """AutoreIndipendente
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Canzoniere
:Canzoniere rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :Contiene ;
                                  owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Poesia
                                ] ;
            rdfs:subClassOf :CollanaEditoriale ;
            rdfs:comment "Una raccolta di poesie che appartengono alla categoria \"Poesia\". Ogni elemento della collezione è un'istanza di \"Poesia\"."@it ;
            rdfs:label "Canzoniere"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#CasaEditrice
:CasaEditrice rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( <http://www.w3.org/ns/prov#Organization>
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :Pubblica ;
                                                           owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf <http://www.w3.org/ns/prov#Organization> ;
              rdfs:comment "Una casa editrice è un'organizzazione che si occupa della pubblicazione e distribuzione di opere letterarie, scientifiche o artistiche, curando la produzione, la stampa e la diffusione dei libri."@it ;
              rdfs:label "CasaEditrice"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Collaboratore
:Collaboratore rdf:type owl:Class ;
               rdfs:subClassOf :Persona ;
               rdfs:comment "Persona che ha contribuito alla Scrittura di un'Edizione non come Autore"@it ;
               rdfs:label "Collaboratore"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#CollanaEditoriale
:CollanaEditoriale rdf:type owl:Class ;
                   rdfs:subClassOf :ListaDiOpereLetterarie ;
                   rdfs:comment "Una raccolta di testi pubblicati da un editore con criteri comuni di genere, tema o formato."@it ;
                   rdfs:label "CollanaEditoriale"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Editore
:Editore rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Persona
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :Pubblica ;
                                                      owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Persona ;
         rdfs:comment """Persona che ha pubblicato un Edizione
"""@it ;
         rdfs:label """Editore
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Genere
:Genere rdf:type owl:Class ;
        rdfs:comment "Il genere di un'opera letteraria è una categoria che raggruppa libri con caratteristiche tematiche o stilistiche simili, come ad esempio fantasy, giallo, romance, storia, horror, ecc. Ogni genere aiuta a identificare il tono, il contenuto e lo stile dell'opera."@it ;
        rdfs:label "Genere"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#GrandeSaga
:GrandeSaga rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :Contiene ;
                                  owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                  owl:onClass <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                ] ;
            rdfs:subClassOf :Saga ;
            rdfs:comment "Una grande saga è una narrazione estesa e complessa che si sviluppa su molteplici opere letterarie, spesso seguendo diversi gruppi di personaggi o intere famiglie, con una trama che si estende su generazioni o eventi di ampia portata."@it ;
            rdfs:label """GrandeSaga
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#ListaDiOpereLetterarie
:ListaDiOpereLetterarie rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :Contiene ;
                                              owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                            ] ;
                        rdfs:subClassOf <http://dbpedia.org/resource/List> .


###  http://www.semanticweb.org/owl/owlapi/turtle#Luogo
:Luogo rdf:type owl:Class ;
       rdfs:comment "Un Luogo, genericamente fisico o immaginario."@it ;
       rdfs:label "Luogo"@it ;
       skos:closeMatch "frbr:Place"@en .


###  http://www.semanticweb.org/owl/owlapi/turtle#LuogoFisico
:LuogoFisico rdf:type owl:Class ;
             rdfs:subClassOf :Luogo ;
             rdfs:comment "Un Luogo Fisico rappresenta un Luogo fisicamente presente, tramite la denominazione corrente o passata."@it ;
             rdfs:label "LuogoFisico"@it ;
             skos:closeMatch "dc:Location"@en .


###  http://www.semanticweb.org/owl/owlapi/turtle#Nazione
:Nazione rdf:type owl:Class ;
         rdfs:subClassOf :LuogoFisico ;
         rdfs:comment "Un'entità nazionale nel presente o passato."@it ;
         rdfs:label "Nazione"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#OperaTeatrale
:OperaTeatrale rdf:type owl:Class ;
               rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
               rdfs:comment "Un'opera teatrale è una forma di opera letteraria scritta per essere rappresentata su un palcoscenico, con personaggi che parlano e interagiscono tra loro secondo una trama."@it ;
               rdfs:label "Operateatrale"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Persona
:Persona rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
         rdfs:comment "Una persona è un individuo umano che svolge un ruolo all'interno di un contesto sociale o professionale, come autore, editore, o altro."@it ;
         rdfs:label "Persona"@it ;
         skos:exactMatch "frbr:Agent"@en .


###  http://www.semanticweb.org/owl/owlapi/turtle#Personaggio
:Personaggio rdf:type owl:Class ;
             rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
             rdfs:comment " Una persona, creatura magica o entità"@it ;
             rdfs:label "Personaggio"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#PersonaggioLibro
:PersonaggioLibro rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :AppartieneA ;
                                        owl:someValuesFrom [ owl:intersectionOf ( <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty :Contiene ;
                                                                                    owl:someValuesFrom owl:Thing
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                      ] ;
                  rdfs:subClassOf :Personaggio ;
                  rdfs:comment " Una persona, creatura magica o entità che appare specificatamente ad un opera letteraria"@it ;
                  rdfs:label "PersonaggioLibro"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#Poesia
:Poesia rdf:type owl:Class ;
        rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
        rdfs:comment "Forma di espressione letteraria che utilizza il linguaggio in modo artistico, spesso caratterizzata da metrica, ritmo e rime."@it ;
        rdfs:label """Poesia
"""@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#RaccoltaDISaggi
:RaccoltaDISaggi rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :Contiene ;
                                       owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                       owl:onClass :Saggio
                                     ] ;
                 rdfs:subClassOf :CollanaEditoriale ;
                 rdfs:comment "Una raccolta di saggi che appartengono alla categoria \"Saggio\". Ogni elemento della collezione è un'istanza di \"Saggio\"."@it ;
                 rdfs:label "RaccoltaDiSaggi"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#RaccoltaDiRacconti
:RaccoltaDiRacconti rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :Contiene ;
                                          owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                          owl:onClass :Racconto
                                        ] ;
                    rdfs:subClassOf :CollanaEditoriale ;
                    rdfs:comment """ Una raccolta di racconti che appartengono alla categoria \"Racconto\". Ogni elemento della collezione è un'istanza di \"Racconto\".
"""@it ;
                    rdfs:label "RaccoltaDiRacconti"@it .


###  http://www.semanticweb.org/owl/owlapi/turtle#RaccoltaTeatrale
:RaccoltaTeatrale rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :Contiene ;
                                        owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                        owl:onClass :OperaTeatrale
                                      ] ;
                  rdfs:subClassOf :CollanaEditoriale ;
                  rdfs:comment "Una raccolta di opere teatrali che appartengono alla categoria \"OperaTeatrale\". Ogni elemento della collezione è un'istanza di \"OperaTeatrale\""@it ;
