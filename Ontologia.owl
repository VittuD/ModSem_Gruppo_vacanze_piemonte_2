xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://litetaryworktopics.com/litetaryworktopics2025#HaProvenienza
:HaProvenienza rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( <http://www.w3.org/ns/prov#Agent>
                                           <http://www.w3.org/ns/prov#Organization>
                                         )
                           ] ;
               rdfs:range :Nazione .


###  https://litetaryworktopics.com/litetaryworktopics2025#appartieneA
:appartieneA rdf:type owl:ObjectProperty ;
             owl:inverseOf :contiene .


###  https://litetaryworktopics.com/litetaryworktopics2025#contiene
:contiene rdf:type owl:ObjectProperty .


###  https://litetaryworktopics.com/litetaryworktopics2025#haAutore
:haAutore rdf:type owl:ObjectProperty ;
          rdfs:domain :Autore ;
          rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  https://litetaryworktopics.com/litetaryworktopics2025#haEditore
:haEditore rdf:type owl:ObjectProperty ;
           owl:inverseOf :pubblicaPerCasaEditrice ;
           rdfs:domain :CasaEditrice ;
           rdfs:range :Editore .


###  https://litetaryworktopics.com/litetaryworktopics2025#haTema
:haTema rdf:type owl:ObjectProperty ;
        rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
        rdfs:range :Tema .


###  https://litetaryworktopics.com/litetaryworktopics2025#haTraduttore
:haTraduttore rdf:type owl:ObjectProperty ;
              rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
              rdfs:range :Traduttore .


###  https://litetaryworktopics.com/litetaryworktopics2025#pubblica
:pubblica rdf:type owl:ObjectProperty ;
          owl:inverseOf :èStatoPubblicatoDa ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :AutoreIndipendente
                                      :Editore
                                    )
                      ] ;
          rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  https://litetaryworktopics.com/litetaryworktopics2025#pubblicaPerCasaEditrice
:pubblicaPerCasaEditrice rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :pubblica ;
                         rdfs:domain :Editore ;
                         rdfs:range :CasaEditrice .


###  https://litetaryworktopics.com/litetaryworktopics2025#vieneUtilizzatoIn
:vieneUtilizzatoIn rdf:type owl:ObjectProperty ;
                   owl:inverseOf <https://litetaryworktopics.com/litetaryworktopics2025##HaGenereLetterario> ;
                   rdfs:domain :Genere ;
                   rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  https://litetaryworktopics.com/litetaryworktopics2025#èAutoreDi
:èAutoreDi rdf:type owl:ObjectProperty ;
           owl:inverseOf :èScrittoDa ;
           rdfs:domain :Autore ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                      <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                    )
                      ] .


###  https://litetaryworktopics.com/litetaryworktopics2025#èScrittoDa
:èScrittoDa rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                        <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                      )
                        ] ;
            rdfs:range :Autore .


###  https://litetaryworktopics.com/litetaryworktopics2025#èStatoPubblicatoDa
:èStatoPubblicatoDa rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( :AutoreIndipendente
                                               :Editore
                                             )
                               ] .


###  https://litetaryworktopics.com/litetaryworktopics2025#èUnEdizioneDi
:èUnEdizioneDi rdf:type owl:ObjectProperty ;
               owl:inverseOf <https://litetaryworktopics.com/litetaryworktopics2025##HaEdizione> ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
               rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  https://litetaryworktopics.com/litetaryworktopics2025#èUtilizzata
:èUtilizzata rdf:type owl:ObjectProperty ;
             owl:inverseOf <https://litetaryworktopics.com/litetaryworktopics2025##HaLingua> ;
             rdfs:domain :Lingua .


###  https://litetaryworktopics.com/litetaryworktopics2025##HaEdizione
<https://litetaryworktopics.com/litetaryworktopics2025##HaEdizione> rdf:type owl:ObjectProperty ;
                                                                    rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
                                                                    rdfs:range <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> .


###  https://litetaryworktopics.com/litetaryworktopics2025##HaGenereLetterario
<https://litetaryworktopics.com/litetaryworktopics2025##HaGenereLetterario> rdf:type owl:ObjectProperty ,
                                                                                     owl:FunctionalProperty ;
                                                                            rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> .


###  https://litetaryworktopics.com/litetaryworktopics2025##HaLingua
<https://litetaryworktopics.com/litetaryworktopics2025##HaLingua> rdf:type owl:ObjectProperty ,
                                                                           owl:FunctionalProperty ;
                                                                  rdfs:range :Lingua .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:DatatypeProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:DatatypeProperty .


###  https://litetaryworktopics.com/litetaryworktopics2025#haDataPubblicazione
:haDataPubblicazione rdf:type owl:DatatypeProperty ;
                     rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
                     rdfs:range xsd:date .


###  https://litetaryworktopics.com/litetaryworktopics2025#haISBN
:haISBN rdf:type owl:DatatypeProperty ;
        rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
        rdfs:range xsd:nonPositiveInteger .


###  https://litetaryworktopics.com/litetaryworktopics2025#haNome
:haNome rdf:type owl:DatatypeProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( <http://www.w3.org/ns/prov#Agent>
                                    <http://www.w3.org/ns/prov#Organization>
                                    :ListaDiOpereLetterarie
                                    <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                  )
                    ] ;
        rdfs:range xsd:string .


###  https://litetaryworktopics.com/litetaryworktopics2025#haPagine
:haPagine rdf:type owl:DatatypeProperty ;
          rdfs:domain <https://litetaryworktopics.com/litetaryworktopics2025##Edizione> ;
          rdfs:range xsd:integer .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/resource/List
<http://dbpedia.org/resource/List> rdf:type owl:Class ;
                                   rdfs:comment "Una lista è una raccolta di elementi, che può contenere oggetti o entità con caratteristiche simili o correlate, come una serie di libri, opere o altri elementi definiti da un tema comune."@it ;
                                   rdfs:label """List
"""@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Autore
:Autore rdf:type owl:Class ;
        rdfs:subClassOf :Persona ;
        rdfs:comment "Un autore è una persona che scrive un'opera letteraria, scientifica o artistica, ed è responsabile della creazione e dell'originalità del contenuto dell'opera."@it ;
        rdfs:label """Autore
"""@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#AutoreIndipendente
:AutoreIndipendente rdf:type owl:Class ;
                    owl:equivalentClass [ owl:intersectionOf ( :Autore
                                                               :Editore
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :pubblica ;
                                                                 owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                               ]
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :èAutoreDi ;
                                                                 owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                               ]
                                                             ) ;
                                          rdf:type owl:Class
                                        ] ;
                    rdfs:subClassOf :Autore ;
                    rdfs:comment "Un autore indipendente è uno scrittore che pubblica e distribuisce le proprie opere senza l'intermediazione di una casa editrice tradizionale, gestendo autonomamente l'intero processo editoriale e promozionale. Questo approccio consente maggiore libertà creativa e guadagni diretti."@it ;
                    rdfs:label """AutoreIndipendente
"""@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#CasaEditrice
:CasaEditrice rdf:type owl:Class ;
              owl:equivalentClass [ owl:intersectionOf ( <http://www.w3.org/ns/prov#Organization>
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty :pubblicaPerCasaEditrice ;
                                                           owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                                owl:onProperty :pubblica ;
                                                                                owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                                              ]
                                                         ]
                                                       ) ;
                                    rdf:type owl:Class
                                  ] ;
              rdfs:subClassOf <http://www.w3.org/ns/prov#Organization> ;
              rdfs:comment "Una casa editrice è un'organizzazione che si occupa della pubblicazione e distribuzione di opere letterarie, scientifiche o artistiche, curando la produzione, la stampa e la diffusione dei libri."@it ;
              rdfs:label "CasaEditrice"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#CollanaEditoriale
:CollanaEditoriale rdf:type owl:Class ;
                   rdfs:subClassOf :ListaDiOpereLetterarie ;
                   rdfs:comment "Una raccolta di testi pubblicati da un editore con criteri comuni di genere, tema o formato."@it ;
                   rdfs:label "CollanaEditoriale"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Editore
:Editore rdf:type owl:Class ;
         owl:equivalentClass [ owl:intersectionOf ( :Persona
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :pubblica ;
                                                      owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##Edizione>
                                                    ]
                                                  ) ;
                               rdf:type owl:Class
                             ] ;
         rdfs:subClassOf :Persona ;
         rdfs:label """Editore
"""@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Genere
:Genere rdf:type owl:Class ;
        rdfs:comment "Il genere di un'opera letteraria è una categoria che raggruppa libri con caratteristiche tematiche o stilistiche simili, come ad esempio fantasy, giallo, romance, storia, horror, ecc. Ogni genere aiuta a identificare il tono, il contenuto e lo stile dell'opera."@it ;
        rdfs:label "Genere"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Lingua
:Lingua rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :It
                                          :En
                                        )
                            ] ;
        rdfs:comment "Sistema linguistico utilizzato nell'opera letteraria, definendo l'idioma in cui il testo è scritto, come italiano, inglese, francese, ecc. Questo elemento permette di associare un'opera alla sua lingua di espressione."@it ;
        rdfs:label "Lingua"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Luogo
:Luogo rdf:type owl:Class ;
       rdfs:comment "Un Luogo, genericamente fisico o immaginario."@it ;
       rdfs:label "Luogo"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#LuogoFisico
:LuogoFisico rdf:type owl:Class ;
             rdfs:subClassOf :Luogo ;
             rdfs:comment "Un Luogo Fisico rappresenta un Luogo fisicamente presente, tramite la denominazione corrente o passata."@it ;
             rdfs:label "LuogoFisico"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Nazione
:Nazione rdf:type owl:Class ;
         rdfs:subClassOf :LuogoFisico ;
         rdfs:comment "Un'entità nazionale nel presente o passato."@it ;
         rdfs:label "Nazione"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#OperaTeatrale
:OperaTeatrale rdf:type owl:Class ;
               rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
               rdfs:comment "Un'opera teatrale è una forma di opera letteraria scritta per essere rappresentata su un palcoscenico, con personaggi che parlano e interagiscono tra loro secondo una trama."@it ;
               rdfs:label "Operateatrale"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Persona
:Persona rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
         rdfs:comment "Una persona è un individuo umano che svolge un ruolo all'interno di un contesto sociale o professionale, come autore, editore, o altro."@it ;
         rdfs:label "Persona"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Poesia
:Poesia rdf:type owl:Class ;
        rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
        rdfs:comment "Forma di espressione letteraria che utilizza il linguaggio in modo artistico, spesso caratterizzata da metrica, ritmo e rime."@it ;
        rdfs:label """Poesia
"""@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Racconto
:Racconto rdf:type owl:Class ;
          rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
          rdfs:comment "Il racconto è un'opera narrativa breve che si concentra su un singolo evento o tema, con un numero limitato di personaggi e uno sviluppo conciso della trama."@it ;
          rdfs:label "Racconto"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Romanzo
:Romanzo rdf:type owl:Class ;
         rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
         rdfs:comment "Il romanzo è un'opera letteraria di narrativa lunga, che sviluppa una trama complessa e approfondisce la caratterizzazione dei personaggi attraverso eventi e situazioni."@it ;
         rdfs:label "Romanzo"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Saga
:Saga rdf:type owl:Class ;
      rdfs:subClassOf :ListaDiOpereLetterarie ;
      rdfs:comment "Una narrazione estesa che segue personaggi o famiglie attraverso eventi o generazioni, spesso con una trama continuativa."@it ;
      rdfs:label "Saga"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Saggio
:Saggio rdf:type owl:Class ;
        rdfs:subClassOf <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria> ;
        rdfs:comment "Il saggio è un'opera letteraria che esamina un argomento in modo critico, riflessivo e approfondito, utilizzando argomentazioni e prove per sostenere un punto di vista."@it ;
        rdfs:label "Saggio"@it .


###  http://www.semanticweb.org/enryc/ontologies/2025/0/untitled-ontology-36#Tema
:Tema rdf:type owl:Class ;
      rdfs:comment "Materia trattata da un'Opera Letteraria."@it ;
      rdfs:label "Tema"@it .


###  http://www.w3.org/ns/prov#Agent
<http://www.w3.org/ns/prov#Agent> rdf:type owl:Class ;
                                  rdfs:comment "Un agente è un'entità che ha un ruolo attivo in un contesto sociale o lavorativo. Può essere un'organizzazione o una persona che svolge attività specifiche come creare, pubblicare o distribuire opere."@it ;
                                  rdfs:label "Agent"@it .


###  http://www.w3.org/ns/prov#Organization
<http://www.w3.org/ns/prov#Organization> rdf:type owl:Class ;
                                         rdfs:subClassOf <http://www.w3.org/ns/prov#Agent> ;
                                         rdfs:comment "Un'organizzazione è un'entità strutturata che agisce come un singolo soggetto, come una casa editrice, un'impresa o un ente, ed è composta da persone che operano insieme per raggiungere obiettivi comuni."@it ;
                                         rdfs:label "Organization"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#Canzoniere
:Canzoniere rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :contiene ;
                                  owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Poesia
                                ] ;
            rdfs:subClassOf :CollanaEditoriale ;
            rdfs:comment "Una raccolta di poesie che appartengono alla categoria \"Poesia\". Ogni elemento della collezione è un'istanza di \"Poesia\"."@it ;
            rdfs:label "Canzoniere"@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#GrandeSaga
:GrandeSaga rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :contiene ;
                                  owl:minQualifiedCardinality "3"^^xsd:nonNegativeInteger ;
                                  owl:onClass <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                ] ;
            rdfs:subClassOf :Saga ;
            rdfs:comment "Una grande saga è una narrazione estesa e complessa che si sviluppa su molteplici opere letterarie, spesso seguendo diversi gruppi di personaggi o intere famiglie, con una trama che si estende su generazioni o eventi di ampia portata."@it ;
            rdfs:label """GrandeSaga
"""@it .


###  https://litetaryworktopics.com/litetaryworktopics2025#ListaDiOpereLetterarie
:ListaDiOpereLetterarie rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Restriction ;
                                              owl:onProperty :contiene ;
                                              owl:someValuesFrom <https://litetaryworktopics.com/litetaryworktopics2025##OperaLetteraria>
                                            ] ;
                        rdfs:subClassOf <http://dbpedia.org/resource/List> .


###  https://litetaryworktopics.com/litetaryworktopics2025#RaccoltaDISaggi
:RaccoltaDISaggi rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :contiene ;
                                       owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                       owl:onClass :Saggio
                                     ] ;
                 rdfs:subClassOf :CollanaEditoriale ;
                 rdfs:comment "Una raccolta di saggi che appartengono alla categoria \"Saggio\". Ogni elemento della collezione è un'istanza di \"Saggio\"."@it ;
