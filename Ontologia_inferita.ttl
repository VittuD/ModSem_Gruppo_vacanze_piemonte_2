@prefix : <https://litetaryworktopics.com/litetaryworktopics2025#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://litetaryworktopics.com/litetaryworktopics2025#> .

<https://litetaryworktopics.com/litetaryworktopics2025#> rdf:type owl:Ontology ;
                                                          <http://purl.org/dc/elements/1.1/creator> "Davide Vitturini"@en ,
                                                                                                    "Enrico Chiesa"@en ;
                                                          <http://purl.org/dc/elements/1.1/date> "2025-01-29" ;
                                                          <http://purl.org/dc/elements/1.1/description> "Questa ontologia ha l'obiettivo di fornire una rappresentazione generale sulle principali tematiche trattate dalla letteratura, con il particolare obiettivo di rendere possibile il suggerimento di nuove da parte di esperti o amatori."@it ;
                                                          <http://purl.org/vocab/vann/preferredNamespacePrefix> "lwt" ;
                                                          <http://purl.org/vocab/vann/preferredNamespaceUri> "https://litetaryworktopics.com/litetaryworktopics2025#"@en ;
                                                          <http://web.resource.org/cc/license> "MIT" ;
                                                          rdfs:label "litetaryworktopics2025"@en .

