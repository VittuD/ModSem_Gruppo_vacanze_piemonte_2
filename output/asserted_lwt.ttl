@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix geo: <https://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix lwt: <https://litetaryworktopics.com/litetaryworktopics2025#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <https://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dbpedia: <https://dbpedia.org/ontology/> .
@prefix schema: <http://schema.org/> .

@base <https://litetaryworktopics.com/litetaryworktopics2025#> .

<https://litetaryworktopics.com/litetaryworktopics2025#> rdf:type owl:Ontology ;
                                                          dc:creator "Davide Vitturini"@en ,
                                                                     "Enrico Chiesa"@en ;
                                                          dc:date "2025-01-29" ;
                                                          dc:description "Questa ontologia ha l_obiettivo di fornire una rappresentazione generale sulle principali tematiche trattate dalla letteratura, con il particolare obiettivo di rendere possibile il suggerimento di nuove da parte di esperti o amatori."@it ;
                                                          vann:preferredNamespacePrefix "lwt" ;
                                                          vann:preferredNamespaceUri "https://litetaryworktopics.com/litetaryworktopics2025#"@en ;
                                                          <http://web.resource.org/cc/license> "MIT" ;
                                                          rdfs:label "litetaryworktopics2025"@en .

